{
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:18889",
  "OTEL_SERVICE_NAME": "cdms-gateway",
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  },
  "Routing": {
    "Routes": [
      {
        "Name": "alvs-ipaffs",
        "RoutedUrlName": "stub",
        "ForkedUrlName": "forked-stub"
      },
      {
        "Name": "cds",
        "RoutedUrlName": "stub",
        "ForkedUrlName": "forked-stub"
      },
      {
        "Name": "test",
        "RoutedUrlName": "stub",
        "ForkedUrlName": "forked-stub"
      },
      {
        "Name": "simulator-alvs-ipaffs",
        "RoutedUrlName": "alvs-ipaffs-simulator",
        "ForkedUrlName": "forked-stub"
      },
      {
        "Name": "simulator-alvs-cds",
        "RoutedUrlName": "alvs-cds-simulator",
        "ForkedUrlName": "forked-stub"
      },
      {
        "Name": "simulator-cds",
        "RoutedUrlName": "cds-simulator",
        "ForkedUrlName": "forked-stub"
      }
    ],
    "NamedUrls": [
      {
        "Name": "stub"
      },
      {
        "Name": "forked-stub"
      },
      {
        "Name": "cds-simulator"
      },
      {
        "Name": "alvs-cds-simulator"
      },
      {
        "Name": "alvs-ipaffs-simulator"
      }
    ],
    "HealthUrls": [
      {
        "Name": "stub",
        "Method": "GET"
      },
      {
        "Name": "cds",
        "Method": "GET"
      },
      {
        "Name": "IPAFFS Static-Vnet",
        "Method": "POST"
      }
    ]
  }
}
