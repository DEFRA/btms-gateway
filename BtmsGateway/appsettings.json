{
  "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
  "OTEL_SERVICE_NAME": "Btms-Gateway",
  "AWS_EMF_NAMESPACE": "BtmsGateway",
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  },
  "Routing": {
    "NamedRoutes": {
      "cds": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "forked-stub",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "alvs-cds": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "forked-stub",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "alvs-ipaffs": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "forked-stub",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "test": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "forked-stub",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "simulator-cds": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "cds-simulator",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "simulator-alvs-cds": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "alvs-cds-simulator",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "simulator-alvs-ipaffs": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "alvs-ipaffs-simulator",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      }
    },
    "NamedLinks": {
      "stub": {
        "LinkType": "Url"
      },
      "forked-stub": {
        "LinkType": "Url"
      },
      "cds-simulator": {
        "LinkType": "Url"
      },
      "alvs-cds-simulator": {
        "LinkType": "Url"
      },
      "alvs-ipaffs-simulator": {
        "LinkType": "Url"
      }
    }
  },
  "HealthCheck": {
    "Disabled": false,
    "Urls": {
      "stub": {
        "Method": "GET"
      },
      "cds": {
        "Method": "GET"
      },
      "IPAFFS_Static_Vnet_Soap_Search": {
        "Method": "POST"
      },
      "IPAFFS_Integration_Notification": {
        "Method": "POST"
      },
      "IPAFFS_Integration_Soap_Search": {
        "Method": "POST"
      },
      "IBM_ALVS_IP": {
        "Method": "GET"
      },
      "IBM_ALVS_DNS": {
        "Method": "GET"
      }
    }
  }
}
