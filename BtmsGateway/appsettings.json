{
  "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
  "OTEL_SERVICE_NAME": "Btms-Gateway",
  "AWS_EMF_NAMESPACE": "BtmsGateway",
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  },
  "Routing": {
    "NamedRoutes": {
      "cds": {
        "LegacyUrlName": "stub",
        "BtmsUrlName": "forked-stub",
        "RouteTo": "Legacy"
      },
      "alvs-cds": {
        "LegacyUrlName": "stub",
        "BtmsUrlName": "forked-stub",
        "RouteTo": "Legacy"
      },
      "alvs-ipaffs": {
        "LegacyUrlName": "stub",
        "BtmsUrlName": "forked-stub",
        "RouteTo": "Legacy"
      },
      "test": {
        "LegacyUrlName": "stub",
        "BtmsUrlName": "forked-stub",
        "RouteTo": "Legacy"
      },
      "simulator-cds": {
        "LegacyUrlName": "stub",
        "BtmsUrlName": "cds-simulator",
        "RouteTo": "Legacy"
      },
      "simulator-alvs-cds": {
        "LegacyUrlName": "stub",
        "BtmsUrlName": "alvs-cds-simulator",
        "RouteTo": "Legacy"
      },
      "simulator-alvs-ipaffs": {
        "LegacyUrlName": "stub",
        "BtmsUrlName": "alvs-ipaffs-simulator",
        "RouteTo": "Legacy"
      }
    },
    "NamedUrls": {
      "stub": "http://btms-gateway-stub.localtest.me:3092/",
      "forked-stub": "http://btms-gateway-stub.localtest.me:3092/forked",
      "cds-simulator": "http://btms-gateway-stub.localtest.me:3092/simulator/cds",
      "alvs-cds-simulator": "http://btms-gateway-stub.localtest.me:3092/simulator/alvs-cds",
      "alvs-ipaffs-simulator": "http://btms-gateway-stub.localtest.me:3092/simulator/alvs-ipaffs"
    }
  },
  "HealthUrls": {
    "stub": {
      "Method": "GET",
      "Url": "http://btms-gateway-stub.localtest.me:3092/health"
    },
    "cds": {
      "Method": "GET",
      "Url": "https://cdcm-eis.cds.dit1.n.mes.corp.hmrc.gov.uk"
    },
    "IPAFFS Static-Vnet": {
      "Method": "POST",
      "Url": "https://importnotification-api-static-snd.azure.defra.cloud/soapsearch/vnet"
    },
    "IPAFFS Static-Test": {
      "Method": "POST",
      "Url": "https://imports-proxy.azurewebsites.net/soapsearch/test"
    },
    "IPAFFS Octopus": {
      "Method": "POST",
      "Url": "https://importnotification-api-tst.azure.defra.cloud/soapsearch/tst"
    }
  }
}
