{
  "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
  "OTEL_SERVICE_NAME": "Btms-Gateway",
  "AWS_EMF_NAMESPACE": "BtmsGateway",
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  },
  "Routing": {
    "NamedRoutes": {
      "cds": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "forked-stub",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "alvs-cds": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "forked-stub",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "alvs-ipaffs": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "forked-stub",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "test": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "forked-stub",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "simulator-cds": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "cds-simulator",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "simulator-alvs-cds": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "alvs-cds-simulator",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      },
      "simulator-alvs-ipaffs": {
        "LegacyLinkName": "stub",
        "BtmsLinkName": "alvs-ipaffs-simulator",
        "SendLegacyResponseToBtms": false,
        "RouteTo": "Legacy"
      }
    },
    "NamedLinks": {
      "stub": {
        "Link": "http://btms-gateway-stub.localtest.me:3092/",
        "LinkType": "Url"
      },
      "forked-stub": {
        "Link": "http://btms-gateway-stub.localtest.me:3092/forked",
        "LinkType": "Url"
      },
      "cds-simulator": {
        "Link": "http://btms-gateway-stub.localtest.me:3092/simulator/cds",
        "LinkType": "Url"
      },
      "alvs-cds-simulator": {
        "Link": "http://btms-gateway-stub.localtest.me:3092/simulator/alvs-cds",
        "LinkType": "Url"
      },
      "alvs-ipaffs-simulator": {
        "Link": "http://btms-gateway-stub.localtest.me:3092/simulator/alvs-ipaffs",
        "LinkType": "Url"
      }
    }
  },
  "HealthCheck": {
    "Disabled": false,
    "Urls": {
      "stub": {
        "Method": "GET",
        "Url": "http://btms-gateway-stub.localtest.me:3092/health"
      },
      "cds": {
        "Method": "GET",
        "Url": "https://cdcm-eis.cds.dit1.n.mes.corp.hmrc.gov.uk"
      },
      "IPAFFS Static-Vnet": {
        "Method": "POST",
        "Url": "https://importnotification-api-static-snd.azure.defra.cloud/soapsearch/vnet"
      },
      "IPAFFS Static-Test": {
        "Method": "POST",
        "Url": "https://imports-proxy.azurewebsites.net/soapsearch/test"
      },
      "IPAFFS Octopus": {
        "Method": "POST",
        "Url": "https://importnotification-api-tst.azure.defra.cloud/soapsearch/tst"
      }
    }
  }
}
