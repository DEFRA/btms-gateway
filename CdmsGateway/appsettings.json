{
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:18889",
  "OTEL_SERVICE_NAME": "cdms-gateway",
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  },
  "Routing": {
    "Routes": [
      {
        "Name": "alvs-ipaffs",
        "RoutedUrlName": "stub",
        "ForkedUrlName": "forked-stub"
      },
      {
        "Name": "cds",
        "RoutedUrlName": "stub",
        "ForkedUrlName": "forked-stub"
      },
      {
        "Name": "alvs-cds",
        "RoutedUrlName": "stub",
        "ForkedUrlName": "forked-stub"
      },
      {
        "Name": "test",
        "RoutedUrlName": "stub",
        "ForkedUrlName": "forked-stub"
      }
    ],
    "NamedUrls": [
      {
        "Name": "stub",
        "Url": "http://cdms-gateway-stub.localtest.me:3092/"
      },
      {
        "Name": "forked-stub",
        "Url": "http://cdms-gateway-stub.localtest.me:3092/forked/"
      }
    ],
    "HealthUrls": [
      {
        "Name": "stub",
        "Method": "GET",
        "Url": "http://cdms-gateway-stub.localtest.me:3092/health"
      },
      {
        "Name": "CDS DIT1 DNS",
        "Method": "GET",
        "Url": "https://cdcm-eis.cds.dit1.n.mes.corp.hmrc.gov.uk"
      },
      {
        "Name": "CDS DIT1 IP",
        "Method": "GET",
        "Url": "https://10.179.33.215"
      },
      {
        "Name": "google",
        "Method": "GET",
        "Url": "https://google.com"
      }
    ]
  }
}
