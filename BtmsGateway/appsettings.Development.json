{
  "AWS": {
    "ServiceURL": "http://sqs.eu-west-2.localhost.localstack.cloud:4566/"
  },
  "AWS_ACCESS_KEY_ID": "local",
  "AWS_SECRET_ACCESS_KEY": "local",
  "AWS_REGION": "eu-west-2",
  "SQS_ENDPOINT": "http://sqs.eu-west-2.localhost.localstack.cloud:4566",
  "AWS_EMF_ENABLED": false,
  "DetailedErrors": true,
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u4}] {Message} {Properties}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Routing": {
    "AutomatedHealthCheckDisabled": false,
    "NamedLinks": {
      "Stub": {
        "Link": "http://localhost:9090/stub/"
      },
      "InboundCustomsDeclarationReceivedTopic": {
        "Link": "arn:aws:sns:eu-west-2:000000000000:trade_imports_inbound_customs_declarations.fifo"
      }
    },
    "Destinations": {
      "BtmsCds": {
        "Link": "http://localhost:5003"
      }
    }
  },
  "HealthCheck": {
    "AutomatedHealthCheckDisabled": false,
    "Urls": {
      "Stub": {
        "Url": "http://localhost:9090/stub/health"
      },
      "HMRC_CDS": {
        "Url": "http://localhost:5003/health",
        "IncludeInAutomatedHealthCheck": true,
        "AdditionalSuccessStatuses": [ 405 ]
      }
    }
  },
  "MessageLoggingOptions": {
    "LogRawMessage": false
  },
  "Acl": {
    "Clients": {
      "Developer": {
        "Secret": "developer-pwd",
        "Scopes": [
          "execute"
        ]
      },
      "IntegrationTests": {
        "Secret": "integration-tests-pwd",
        "Scopes": [
          "execute"
        ]
      }
    }
  },
  "HttpClientTimeoutSeconds": 3,
  "CdsHttpClientRetries": 1,
  "AwsSqsOptions": {
    "Topics": [ "arn:aws:sns:eu-west-2:000000000000:trade_imports_btms_activity", "arn:aws:sns:eu-west-2:000000000000:trade_imports_inbound_customs_declarations.fifo" ]
  }
}
